{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/electron.js",
  "scripts": {
    "dev1": "vite",
    "build1": "tsc -b && vite build",
    "lint1": "eslint .",
    "preview1": "vite preview",

    "start": "concurrently \"npm run frontend\" \"sleep 2 && electron .\"",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" \"sleep 2 && npm run electron",
    "frontend": "vite",
    "backend": "cd ../backend && mvn spring-boot:run",
    "electron": "tsc -b && electron dist",
    "build": "vite build && electron-builder"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron": "^34.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
